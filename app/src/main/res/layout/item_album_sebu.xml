<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/back"
        android:visibility="invisible"
        android:layout_width="@dimen/dimen_260"
        android:layout_height="@dimen/dimen_100"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/dimen_96"
        android:layout_marginTop="@dimen/dimen_96"
        android:src="@drawable/kyodong_img_005_05"
        />

    <ImageView
        android:id="@+id/back_img"
        android:layout_width="@dimen/dimen_260"
        android:layout_height="@dimen/dimen_100"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/dimen_96"
        android:src="@drawable/kyodong_img_005_05"
        />

    <TextView
        android:id="@+id/year_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@id/back_img"
        app:layout_constraintEnd_toEndOf="@id/back_img"
        app:layout_constraintTop_toTopOf="@id/back_img"
        app:layout_constraintBottom_toBottomOf="@id/back_img"
        android:textSize="@dimen/font_52"
        android:textColor="#ffffff"
        android:fontFamily="@font/ridibatang"
        />

</androidx.constraintlayout.widget.ConstraintLayout>